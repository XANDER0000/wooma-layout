{
  "name": "vavu",
  "version": "1.1.0",
  "description": "Интернет магазин 'VAVU'",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "gulp",
    "build": "cross-env NODE_ENV=production gulp",
    "start": "cross-env NODE_ENV=production gulp",
    "zip": "gulp zip",
    "setup": "npm install",
    "component": "node add-component",
    "eslint": "eslint ./ --ext .js",
    "pug-lint": "pug-lint --reporter node_modules/puglint-stylish src",
    "stylelint": "stylelint \"src/**/*.{styl,stylus}\"",
    "validate-all": "html-validator dist --trim=30 --continue --quiet",
    "validate": "html-validator --trim=30 --continue --quiet",
    "prepare": "husky",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.{js,.ts,.json}": "eslint",
    "*.{pug,jade}": "pug-lint",
    "*.{styl,stylus}": "stylelint"
  },
  "main": "./gulpfile.js",
  "devDependencies": {
