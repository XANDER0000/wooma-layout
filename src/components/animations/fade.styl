@keyframes fadeIn
  from
    opacity 0
  to
    opacity 1

@keyframes fadeOut
  from
    opacity 1
  to
    opacity 0


@keyframes fadeInDown
  from
    transform translate3d(0, -100%, 0)
    opacity 0
  to
    transform none
    opacity 1

@keyframes fadeInUp
  from
    transform translateY(20px)
    opacity 0
  to
    transform translateY(0)
    opacity 1
